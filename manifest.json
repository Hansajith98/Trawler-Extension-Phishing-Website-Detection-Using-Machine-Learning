{
    "manifest_version": 2,
    "name": "Extension1",
    "version": "1.0",
    "content_scripts" : [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage",
        "notifications"
    ],
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "cs.ico",
        "default_popup": "popup.html",
        "default_title": "A popup will come here"
    }
}